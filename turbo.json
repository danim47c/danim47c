{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "codegen": {},
    "deploy": {
      "env": [
        "DOMAIN_NAME"
      ],
      "dependsOn": [
        "build"
      ]
    },
    "deploy:remove": {
      "env": [
        "DOMAIN_NAME"
      ]
    },
    "build": {
      "dependsOn": [
        "lint",
        "^build"
      ],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": [
        ".eslintcache"
      ]
    },
    "dev": {
      "cache": false
    }
  }
}
